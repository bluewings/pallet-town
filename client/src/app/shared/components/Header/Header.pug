  .container-fluid
    .row
      .visible-xs.col-xs-3(styleName='col-navbar-toggle')
        button.navbar-toggle(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar', onClick='{this.props.toggleAside.bind(this)}')
